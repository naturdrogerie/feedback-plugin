<script type="text/javascript">
    window.feedbackPublicPath = "{{ plugin_path('Feedback') ~ '/js/dist/' }}"
</script>

{% set minSuffix = '.min' %}
{% if ceresConfig.log.performanceLevel  == 'development' %}
